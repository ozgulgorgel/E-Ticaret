
@model IEnumerable<E_TicaretMVC.Models.Marka>


<div class="w3ls_mobiles_grid_left_grid">
    <h3>Markalar</h3>
    <div class="w3ls_mobiles_grid_left_grid_sub">
       
        <ul class="panel_bottom">
            @foreach (var item in Model)
            {
                <li><a href="/Home/MarkalaraGoreUrunler/@item.MarkaID">@item.Adi (@item.Uruns.Count)</a></li>
            }
        </ul>
    </div>
</div>

